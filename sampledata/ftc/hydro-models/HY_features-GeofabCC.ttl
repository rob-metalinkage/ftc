@base <http://id.sirf.net/def/schema/source/> .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix h2o: <http://def.seegrid.csiro.au/isotc211/iso19150/-2/2012/basic#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix basic: <http://def.seegrid.csiro.au/isotc211/iso19103/2005/basic#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix gml: <http://def.seegrid.csiro.au/isotc211/iso19150/-2/2012/gml#> .

@prefix skosm: <http://www.w3.org/2004/02/skos/mapping#> .
#@prefix ftcm: <http://def.seegrid.csiro.au/def/ftc#> .

@prefix ftcm: <http://id.sirf.net/def/schema/meta-modelmappings/> .

@prefix   idp: <http://demo.metalinkage.com.au/def/schema/hy_features/abstracthydrofeature/> .
# note that this is used for a named graph to select mappings defined in this file, exclusing other mappings that map exist.
# it must be used as the target context in deployment.csv, without the trialing slash
@prefix  idpg: <http://demo.metalinkage.com.au/def/schema/source/HY_HydroFeature-AHGF_HR_Catchments/> .
@prefix gaz: <http://pid.ereefs.info/def/schema/ahgf/HR_Catchments/> .

idpg:shape a ftcm:PropertyMapping;
    ftcm:mappingScope idpg:AHGFContractedCatchment;
    ftcm:sourceFeature idp:HY_CatchmentArea ;
    ftcm:targetFeature gaz:AHGFContractedCatchment ;
    ftcm:property idp:shape ;
    ftcm:gml_xpath "shape" ;
    ftcm:target gaz:shape ;
    rdfs:comment "This dependency was generated by Solid Ground - None function" ;
    .

idpg:AHGFContractedCatchment  a ftcm:MappingClass ;
    ftcm:sourceFeature idp:HY_Basin ;
    ftcm:targetFeature gaz:AHGFContractedCatchment ;
    rdfs:comment "" ;
    .

idpg:hydroID a ftcm:PropertyMapping;
    ftcm:mappingScope idpg:AHGFLink;
    ftcm:sourceFeature idp:HY_Basin ;
    ftcm:targetFeature gaz:AHGFLink ;
    ftcm:property idp:code ;
    ftcm:gml_xpath "code" ;
    ftcm:target gaz:hydroID ;
    rdfs:comment "This dependency was generated by Solid Ground - None function" ;
    .

idpg:AHGFLink  a ftcm:MappingClass ;
    ftcm:sourceFeature idp:HY_Basin ;
    ftcm:targetFeature gaz:AHGFLink ;
    rdfs:comment "" ;
    .

idpg:conNodeId a ftcm:PropertyMapping;
    ftcm:mappingScope idpg:AHGFNode;
    ftcm:sourceFeature idp:HY_Outfall ;
    ftcm:targetFeature gaz:AHGFNode ;
    ftcm:property idp:identifier ;
    ftcm:gml_xpath "gml:name" ;
    ftcm:target gaz:conNodeId ;
    rdfs:comment "" ;
    .

idpg:nextDownID a ftcm:PropertyMapping;
    ftcm:mappingScope idpg:AHGFNode;
    ftcm:sourceFeature idp:HY_Outfall ;
    ftcm:targetFeature gaz:AHGFNode ;
    ftcm:property idpg:nextDownIDPropertyPath ;
#    ftcm:gml_xpath "receivingBasin.outflowNode" ;
    ftcm:target gaz:nextDownID ;
    rdfs:comment "" ;
    .

idpg:nextDownIDPropertyPath  a ftcm:propertyPath ;
	ftcm:path1 idp:receivingbasin ;
	ftcm:path2 idp:outflownode ;		
.

idpg:position a ftcm:PropertyMapping;
    ftcm:mappingScope idpg:AHGFNode;
    ftcm:sourceFeature idp:HY_Outfall ;
    ftcm:targetFeature gaz:AHGFNode ;
    ftcm:property idpg:positionPropertyPath ;
#    ftcm:gml_xpath "position.referencePoint.refPoint" ;
    ftcm:target gaz:position ;
#    rdfs:comment "This dependency was generated by Solid Ground - None function" ;
    .

idpg:positionPropertyPath  a ftcm:propertyPath ;
	ftcm:path1 idp:position ;
	ftcm:path2  idp:referencepoint ;
	ftcm:path3 idp:refpoint 
.

idpg:AHGFNode  a ftcm:MappingClass ;
    ftcm:sourceFeature idp:HY_Outfall ;
    ftcm:targetFeature gaz:AHGFNode ;
    rdfs:comment "" ;
    .

